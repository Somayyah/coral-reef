{
  "name": "coral-reef",
  "version": "1.0.2",
  "description": "A powerfull Next.js setup with so many no conf features",
  "main": "src/index.js",
  "scripts": {
	"start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
  	"create": "npm run build && npm run test",
  	"build": "tsc -p .",
  	"local": "sudo npm i -g && coral",
	"refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "bin": {
    "coral": "./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Somayyah/coral-reef.git"
  },
  "keywords": [
    "template",
    "next.js",
    "typescript",
    "tailwind",
    "minify-css",
    "minify-js",
    "minify-html",
    "automate"
  ],
  "author": "Somayyah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Somayyah/coral-reef/issues"
  },
  "homepage": "https://github.com/Somayyah/coral-reef#readme",
  "dependencies": {
    "@types/react": "^17.0.1",
    "autoprefixer": "^10.0.4",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.0",
    "next": "latest",
    "next-seo": "^4.19.0",
    "path": "^0.12.7",
    "postcss": "^8.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
